#include<stdio.h>
#include<conio.h>
main()
{ int a[10],i,max,min,n;
printf("enter the no. of element in array");
scanf("%d",&n);
printf("enter the elements of array");
for(i=0;i<n;i++)
{ scanf("%d",&a[i]);
}
max=min=a[0];
for(i=0;i<n;i++)
{if(max<a[i])
max=a[i];
if(min>a[i])
min=a[i];
}
printf("max is %d and min is %d",max,min);
getch();
}