#include<stdio.h>
#include<conio.h>

int add(int *num1, int *num2)
{ int temp;
temp=*num1+*num2;
return (temp);
}

void main() {
   int *x, *y;
   int add(int *,int *);

   printf("\nEnter two no. for addition: ");
   scanf("%d%d",&x, &y);
   int m=add(&x,&y);
   printf("\nsum of  %d and  %d is %d", x, y,m);
   getch();
}